(indicator);\n    if (window.screen.width < 768) {\n      showPreview.click();\n    }\n  });</script>","id":"Cent-å·²æ”¯æŒå¤šå¸ç§è‡ªåŠ¨è®°è´¦","path":"/posts/Cent-å·²æ”¯æŒå¤šå¸ç§è‡ªåŠ¨è®°è´¦.json"}